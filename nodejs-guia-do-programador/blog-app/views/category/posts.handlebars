<h1>{{category.name}}</h1>
{{#each posts}}
  <div class="card mt-4">
    <div class="card-body">
      <h3>{{title}}</h3>
      <p>{{content}}</p>
      <a href="/post/{{slug}}"><button class="btn btn-success">Leia mais</button></a><br
      /><br />
      <small>Data de publicação: {{date}}</small>
    </div>
  </div>
{{else}}
  <h5>Nenhuma postagem</h5>
{{/each}}